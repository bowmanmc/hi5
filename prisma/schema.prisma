
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Hi5 {
    id    Int     @default(autoincrement()) @id
    recipient String
    author String
    category String
    description String
    likes String?
}

model User {
    email  String @id
    name   String
    pic    String
}
